<Window x:Class="PassParamWindowsApp.Views.FeatureWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="Feature Window" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBlock Height="25" Margin="100,0,20,0">Received Message:</TextBlock>
            <TextBlock Text="{Binding Message}" Width="100" Height="25" />
        </StackPanel>
    </Grid>
</Window>